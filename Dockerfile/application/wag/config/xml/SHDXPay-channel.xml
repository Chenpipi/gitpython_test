<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="SHDXPay" class="com.bestv.wag.paychannel.shdxpay.SHDXPay"
		parent="payBase">
		<property name="platformid">
			<value>${shdx.pay.platformid}</value>
		</property>
		<property name="applicationid">
			<value>${shdx.pay.applicationid}</value>
		</property>
		<property name="cpid">
			<value>${shdx.pay.cpid}</value>
		</property>
		<property name="apppakagename">
			<value>${shdx.pay.apppakagename}</value>
		</property>
		<property name="appclassname">
			<value>${shdx.pay.appclassname}</value>
		</property>
		<property name="timeout">
			<value>${shdx.pay.timeout}</value>
		</property>
	</bean>
</beans>