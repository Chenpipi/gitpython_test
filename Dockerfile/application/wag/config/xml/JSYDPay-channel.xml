<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="JSYDPay" class="com.bestv.wag.paychannel.jsydpay.JSYDPay" parent="payBase">
		<property name="businesId">
			<value>${jsyd.pay.businesId}</value>
		</property>
		<property name="merchId">
			<value>${jsyd.pay.merchId}</value>
		</property>
		<property name="serviceId">
			<value>${jsyd.pay.serviceId}</value>
		</property>
		<property name="appKey">
			<value>${jsyd.pay.appKey}</value>
		</property>
		<property name="requrl">
			<value>${jsyd.pay.requrl}</value>
		</property>
		<property name="bimsurl">
			<value>${jsyd.bims.url}</value>
		</property>
		<property name="templateEngine">
			<ref bean="templateEngine"></ref>
		</property>		
		<property name="httpClient">
			<ref bean="httpClient"></ref>
		</property>		
		<property name="slaveModuleDao">
			<ref bean="slaveModuleDao"></ref>
		</property>
	</bean>
</beans>